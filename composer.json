{
    "name": "notifymehq/ballou",
    "description": "Provides a ballou gateway for notifyme",
    "license": "MIT",
    "keywords": ["notifymehq", "notifyme", "ballou"],
    "authors": [
        {
            "name": "James Brooks",
            "email": "jbrooksuk@me.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~5.0",
        "notifymehq/notifyme": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "NotifyMeHQ\\Ballou\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotifyMeHQ\\Ballou\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
